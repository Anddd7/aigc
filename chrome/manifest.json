{
    "manifest_version": 3,
    "name": "Prompt Clipboard",
    "version": "1.0.2",
    "description": "create, edit, manage your prompts, oneclick copy",
    "background": {
        "service_worker": "service-worker.js"
    },
    "action": {
        "default_title": "Click to open panel"
    },
    "permissions": [
        "sidePanel",
        "tabs",
        "storage",
        "clipboardWrite"
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    },
    "icons": {
        "64": "images/icon-64.png"
    }
}